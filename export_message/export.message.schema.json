                    },

                    "manufacturer": {
                        "type": "string",
                        "description": "生产厂家"
                    },
                    "quantity": {
                        "type": "number",
                        "description": "法定数量",
                        "minimum": 0
                    },
                    "unit": {
                        "type": "string",
                        "description": "法定单位"
                    },
                    "currency": {
                        "type": "string",
                        "description": "币制"
                    },
                    "unit_price": {
                        "type": "string",
                        "description": "成交单价"
                    },
                    "total_price": {
                        "type": "string",
                        "description": "成交总价"
                    }
                }
            }
        },

        "port_of_export": {
            "type": "string",
            "description": "出口口岸"
        },
        "port_of_destination": {
            "type": "string",
            "description": "指运港"
        },
        "customs_house": {
            "type": "string",
            "description": "申报地海关"
        }
