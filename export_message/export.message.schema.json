{
    "title": "Export",
    "properties": {
        "entry_number": {
            "type": "string",
            "description": "报关单号"
        },
        "date": {
            "type": "string"
        },
        "exporter_name": {
            "type": "string",
            "description": "经营单位"
        },
        "exporter_code": {
            "type": "string",
            "description": "经营单位编码"
        },
        "agent_name": {
            "type": "string",
            "description": "申报单位"
        },
        "agent_code": {
            "type": "string",
            "description": "申报单位编码"
        },
        "final_destination": {
            "type": "string",
            "description": "运抵国（地区）"
        },
        "transport_mode": {
            "type": "string",
            "description": "运输方式"
        },
        "carrier_name": {
            "type": "string",
            "description": "运输工具名称"
        },
        "shipment_reference_num": {
            "type": "string",
            "description": "航次号"
        },
        "contract_num": {
            "type": "string",
            "description": "合同协议号"
        },
        "container_id": {
            "type": "string",
            "description": "集装箱号"
        },
        "trade_mode": {
            "type": "string",
            "description": "监管方式"
        },
        "manifest": {
            "type": "array",
